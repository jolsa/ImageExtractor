{
    "manifest_version": 2,

    "name": "Image Retrieval",
    "description": "Get Images from the current page",
    "version": "1.0.0.5",

    "browser_action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions":
    [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
