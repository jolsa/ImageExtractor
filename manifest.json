{
	"manifest_version": 3,

	"name": "Image Retrieval",
	"description": "Get Images from the current page",
	"version": "1.0.2.9",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Image Retrieval"
	},
	"permissions": [
		"activeTab",
		"downloads",
		"scripting"
	]
}
