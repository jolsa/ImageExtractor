{
	"manifest_version": 2,

	"name": "Image Retrieval",
	"description": "Get Images from the current page",
	"version": "1.0.1.1",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}
